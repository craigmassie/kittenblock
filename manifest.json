{
  "manifest_version": 2,

  "name": "Kitten Block",
  "description": "Redirects the Daily Mail and Daily Express websites to Tea and Kittens.",
  "version": "1.1",
  "background": {
    "scripts": ["kblock.js"],
    "persistent": false
  },
  "permissions": [
    "tabs"
  ],
  "icons": { "48": "kb_48.png", 
             "128": "kb_128.png" }          
}